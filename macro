'Code simply copies data from a pull of sold products to a weekly updated linked BI sheet for daily update

Sub COPYTOTAFDSALESANALYSIS()
Dim WB As Workbook
Dim WS As Worksheet
Dim WBNAME As String

WBNAME = "Tasty distribution sales"


'Barking
For Each WB In Application.Workbooks
    If WB.Name Like WBNAME & "*" Then
    WB.Activate
    End If
Next

Worksheets("BARKING").Activate
Range("A3:A92").Copy
Workbooks("Total sales analysis for despatch").Activate
Range("C1").End(xlDown).Offset(1, -1).Select
ActiveCell.PasteSpecial xlPasteValues

For Each WB In Application.Workbooks
    If WB.Name Like WBNAME & "*" Then
    WB.Activate
    End If
Next

Worksheets("BARKING").Activate
Range("AA3:AA92").Copy
Workbooks("Total sales analysis for despatch").Activate
Range("C1").End(xlDown).Offset(1, 1).Select
ActiveCell.PasteSpecial xlPasteValues

ActiveCell.End(xlDown).Offset(0, -1).Select
Range(Selection, Selection.End(xlUp).Offset(1, 0)).Value = "Barking"


'Belvedere
For Each WB In Application.Workbooks
    If WB.Name Like WBNAME & "*" Then
    WB.Activate
    End If
Next

Worksheets("BELVEDERE").Activate
Range("A3:A92").Copy
Workbooks("Total sales analysis for despatch").Activate
Range("C1").End(xlDown).Offset(1, -1).Select
ActiveCell.PasteSpecial xlPasteValues

For Each WB In Application.Workbooks
    If WB.Name Like WBNAME & "*" Then
    WB.Activate
    End If
Next

Worksheets("BELVEDERE").Activate
Range("AA3:AA92").Copy
Workbooks("Total sales analysis for despatch").Activate
Range("C1").End(xlDown).Offset(1, 1).Select
ActiveCell.PasteSpecial xlPasteValues

ActiveCell.End(xlDown).Offset(0, -1).Select
Range(Selection, Selection.End(xlUp).Offset(1, 0)).Value = "Belvedere"


